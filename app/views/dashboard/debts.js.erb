<% if @print %>
  $('table.resource-list.expired tbody').html('<%= j render(@fees) %>');
  window.print();
<% else %>
  $('table.resource-list.expired tbody').append('<%= j render(@fees) %>');
<% end %>

<% if !@print && @fees.next_page %>
  $('.pagination-container').replaceWith('<%= j pagination_links(@fees) %>');
<% else %>
  $('.pagination-container').remove();
<% end %>
