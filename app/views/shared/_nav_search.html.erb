<% classes = ['navbar-search', 'pull-left', 'not-printable'] %>
<% classes << 'hidden' unless @searchable %>
<%= form_for :search, method: :get, html: { class: classes.join(' ') } do |f| %>
  <%= text_field_tag :q, params[:q], class: 'search-query span2', placeholder: t('label.search') %>
<% end %>
