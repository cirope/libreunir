$('[data-schedulable]').remove();

<% if @schedulable %>
  $('[data-object-id="<%= @schedulable.to_param %>"]').after(
    '<%= j render 'schedulable' %>'
  );
<% else %>
  $('[data-schedule-place-holder]').html(
    '<%= j render(template: 'schedules/new', formats: [:html]) %>'
  );
<% end %>

<%= render 'datepicker_setup' %>
