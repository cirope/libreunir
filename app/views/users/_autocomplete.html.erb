<% field_classes = ['autocomplete_field'] %>
<%= f.input :auto_user_name, label: t('view.users.name'),
  placeholder: User.human_attribute_name('user_id'),
  input_html: {
    id: "auto_user_id",
    class: field_classes.join(' '),
    data: {
      'autocomplete-url' => relatives_users_path(format: :json),
      'autocomplete-id-target' => "#user_id"
    }
  }
  %>
<%= f.input :user_id, as: :hidden,
  input_html: { class: 'autocomplete_id', id: "user_id" }
%>
