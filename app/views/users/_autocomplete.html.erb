<% field_classes = ['autocomplete_field', 'span12'] %>
<%= f.input(
  :auto_user_name,
  as: :fake,
  placeholder: User.human_attribute_name('auto_user_name'),
  input_html: {
    class: field_classes.join(' '),
    data: {
      'autocomplete-url' => relatives_users_path(format: :json),
      'autocomplete-id-target' => '#user_id'
    }
  }
) %>
<%= hidden_field_tag :user_id, nil, id: 'user_id' %>
